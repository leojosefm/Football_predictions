<script>function reloadfix(stage,eq,md,pa,vn){var ckfil=0;var o=document.getElementById('cklnp'); if (o && o.checked ) ckfil=1;  var x=''; if(stage) x= sepvar(x) + 'fst=' + stage; if(eq) x= sepvar(x) + 'feq=' + eq; if(ckfil) x= sepvar(x) + 'fln=' + ckfil; if(md) x= sepvar(x) + 'fmd=' + md; if(pa) x= sepvar(x) + 'fpa=' + pa; if(vn) x= sepvar(x) + 'fpv=' + vn;  window.location = x; }function changestag(stage){var md=document.getElementById('mdxfil'); var o=document.getElementById('eqxfil'); var pa=document.getElementById('paxfil'); var vn=document.getElementById('venuexfil'); if(o && md) { reloadfix(stage,o.value,md.value,pa.value,vn.value); } }function changeeq(eq){var md=document.getElementById('mdxfil'); var o=document.getElementById('stagexfil'); document.getElementById('paxfil').value=''; var vn=document.getElementById('venuexfil'); if(o && md) { reloadfix(o.value,eq,md.value,'',vn.value); } }function changepa(pa){var o=document.getElementById('stagexfil'); var md=document.getElementById('mdxfil'); document.getElementById('eqxfil').value=''; var vn=document.getElementById('venuexfil'); if(md && pa) { reloadfix(o.value,'',md.value,pa,vn.value); } }function changecklnp(){var md=document.getElementById('mdxfil'); var oe=document.getElementById('eqxfil'); var pa=document.getElementById('paxfil'); var os=document.getElementById('stagexfil'); var vn=document.getElementById('venuexfil'); if( oe && os && md) { reloadfix(os.value,oe.value,md.value,pa.value,vn.value); } }function changemd(md){var oe=document.getElementById('eqxfil'); var pa=document.getElementById('paxfil'); var os=document.getElementById('stagexfil'); var vn=document.getElementById('venuexfil'); if( oe && os  ) { reloadfix(os.value,oe.value,md,pa.value,vn.value); } }function changemd2(md){if(md) window.location='?fmd=' + md; }function changevn(vn){var md=document.getElementById('mdxfil'); var oe=document.getElementById('eqxfil'); var pa=document.getElementById('paxfil'); var os=document.getElementById('stagexfil'); if( oe && os  ) { reloadfix(os.value,oe.value,md.value,pa.value,vn); } }</script>
